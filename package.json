{
  "name": "mentorseas-website",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "start": "parcel ./src/index.html",
    "build": "yarn && parcel build ./src/index.html -d build/",
    "test": "yarn && yarn lint && yarn flow",
    "transfer": "scp -r build/* simonz@lnxsrv09.seas.ucla.edu:/usr/public/www/mentorship",
    "deploy": "yarn build && yarn transfer",
    "lint": "eslint src/",
    "lint-fix": "eslint --fix src/",
    "format": "prettier --write \"src/**/*.{js,jsx}\"",
    "flow": "flow status",
    "prepush": "yarn test"
  },
  "dependencies": {
    "eslint-config-react-app": "^1.0.5",
    "eslint-loader": "1.7.1",
    "eslint-plugin-flowtype": "2.34.0",
    "radium": "^0.19.4",
    "react": "^15.6.1",
    "react-dom": "^15.6.1",
    "react-router": "^4.1.2",
    "react-router-dom": "^4.1.2",
    "styled-components": "^3.2.6"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.4",
    "@babel/plugin-transform-flow-strip-types": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "@babel/preset-flow": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-exponentiation-operator": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "eslint": "^3.19.0 || ^4.3.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-react": "^7.1.0",
    "flow-bin": "^0.53.1",
    "husky": "^0.14.3",
    "json5": "^2.1.0",
    "parcel-bundler": "^1.12.3",
    "parcel-plugin-static-files-copy": "^2.0.0",
    "prettier": "^1.12.1",
    "sass": "^1.19.0"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "eslintIgnore": [
    "src/pages/StaffPage/StaffPage.js",
    "src/components/StaffMember"
  ],
  "homepage": "http://mentorship.seas.ucla.edu",
  "prettier": {
    "singleQuote": true,
    "semi": false,
    "printWidth": 120,
    "tabWidth": 2
  },
  "staticFiles": {
    "staticPath": "src/static",
    "watcherGlob": "**"
  }
}
